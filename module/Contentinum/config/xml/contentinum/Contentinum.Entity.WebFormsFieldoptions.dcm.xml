<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contentinum\Entity\WebFormsFieldoptions" table="web_forms_fieldoptions">
    <indexes>
      <index name="FORMFIELDREF" columns="form_field_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="NONE"/>
    </id>
    <field name="optionGroup" type="string" column="option_group" length="250" nullable="false"/>
    <field name="optionValue" type="string" column="option_value" length="250" nullable="false"/>
    <field name="optionLabel" type="string" column="option_label" length="250" nullable="false"/>
    <field name="optionLabeltitle" type="string" column="option_labeltitle" length="250" nullable="false"/>
    <field name="imgSource" type="string" column="img_source" length="250" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <field name="updateBy" type="integer" column="update_by" nullable="false"/>
    <field name="registerDate" type="datetime" column="register_date" nullable="false"/>
    <field name="upDate" type="datetime" column="up_date" nullable="false"/>
    <many-to-one field="formField" target-entity="Contentinum\Entity\WebFormsField">
      <join-columns>
        <join-column name="form_field_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
