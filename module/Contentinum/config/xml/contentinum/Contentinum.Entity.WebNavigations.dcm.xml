<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contentinum\Entity\WebNavigations" table="web_navigations">
    <unique-constraints>
      <unique-constraint name="NAVIGATION" columns="tree_ident"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="NONE"/>
    </id>
    <field name="treeIdent" type="string" column="tree_ident" length="250" nullable="false"/>
    <field name="tplAssign" type="string" column="tpl_assign" length="25" nullable="false"/>
    <field name="menue" type="string" column="menue" length="3" nullable="false"/>
    <field name="title" type="string" column="title" length="150" nullable="false"/>
    <field name="headline" type="string" column="headline" length="80" nullable="false"/>
    <field name="tags" type="string" column="tags" length="2" nullable="false"/>
    <field name="params" type="text" column="params" nullable="false"/>
    <field name="publish" type="string" column="publish" length="10" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <field name="updateBy" type="integer" column="update_by" nullable="false"/>
    <field name="registerDate" type="datetime" column="register_date" nullable="false"/>
    <field name="upDate" type="datetime" column="up_date" nullable="false"/>
  </entity>
</doctrine-mapping>
