<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contentinum\Entity\WebNavigationTree" table="web_navigation_tree">
    <indexes>
      <index name="PAGES" columns="web_pages_id"/>
      <index name="PARENTFROM" columns="parent_from"/>
      <index name="TREES" columns="web_navigation_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="NONE"/>
    </id>
    <field name="webNavigationId" type="integer" column="web_navigation_id" nullable="false"/>
    <field name="parentFrom" type="integer" column="parent_from" nullable="false"/>
    <field name="itemRang" type="integer" column="item_rang" nullable="false"/>
    <field name="relLink" type="string" column="rel_link" length="50" nullable="false"/>
    <field name="class" type="string" column="class" length="50" nullable="false"/>
    <field name="params" type="text" column="params" nullable="false"/>
    <field name="publish" type="string" column="publish" length="10" nullable="false"/>
    <field name="deleted" type="boolean" column="deleted" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <field name="updateBy" type="integer" column="update_by" nullable="false"/>
    <field name="registerDate" type="datetime" column="register_date" nullable="false"/>
    <field name="upDate" type="datetime" column="up_date" nullable="false"/>
    <many-to-one field="webPages" target-entity="Contentinum\Entity\WebPages">
      <join-columns>
        <join-column name="web_pages_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
