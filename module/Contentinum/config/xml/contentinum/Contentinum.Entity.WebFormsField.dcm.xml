<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contentinum\Entity\WebFormsField" table="web_forms_field">
    <indexes>
      <index name="FORMREF" columns="web_forms_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="NONE"/>
    </id>
    <field name="label" type="string" column="label" length="250" nullable="false"/>
    <field name="fieldtyp" type="string" column="fieldtyp" length="12" nullable="false"/>
    <field name="fieldname" type="string" column="fieldname" length="50" nullable="false"/>
    <field name="fieldvalue" type="string" column="fieldvalue" length="250" nullable="false"/>
    <field name="size" type="string" column="size" length="10" nullable="false"/>
    <field name="maxlength" type="string" column="maxlength" length="10" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="selectOptions" type="text" column="select_options" nullable="false"/>
    <field name="images" type="string" column="images" length="250" nullable="false"/>
    <field name="itemRang" type="integer" column="item_rang" nullable="false"/>
    <field name="status" type="string" column="status" length="8" nullable="false"/>
    <field name="publish" type="string" column="publish" length="10" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <field name="updateBy" type="integer" column="update_by" nullable="false"/>
    <field name="registerDate" type="datetime" column="register_date" nullable="false"/>
    <field name="upDate" type="datetime" column="up_date" nullable="false"/>
    <many-to-one field="webForms" target-entity="Contentinum\Entity\WebForms">
      <join-columns>
        <join-column name="web_forms_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
