    <field name="imgAlt2" type="string" column="img_alt2" length="255" nullable="false"/>
    <field name="imgTitle2" type="string" column="img_title2" length="255" nullable="false"/>
    <field name="imgCaption2" type="string" column="img_caption2" length="255" nullable="false"/>
