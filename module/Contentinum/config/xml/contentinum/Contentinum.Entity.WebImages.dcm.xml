    <field name="imgLongdesc" type="text" column="img_longdesc" nullable="true"/>
    <field name="imgLinkTarget" type="string" column="img_link_target" length="4" nullable="false"/>
    <field name="imgLink" type="string" column="img_link" length="255" nullable="false"/>
    <field name="imgLinkRel" type="string" column="img_link_rel" length="10" nullable="false"/>
    <field name="imgAlign2" type="string" column="img_align2" length="25" nullable="false"/>
