<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contentinum\Entity\FieldTypeMetas" table="field_type_metas">
    <indexes>
      <index name="FIELDTYPEREF" columns="field_types_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="DATASCOPE" columns="datascope"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="NONE"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <field name="datascope" type="string" column="datascope" length="60" nullable="false"/>
    <field name="params" type="text" column="params" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <field name="updateBy" type="integer" column="update_by" nullable="false"/>
    <field name="registerDate" type="datetime" column="register_date" nullable="false"/>
    <field name="upDate" type="datetime" column="up_date" nullable="false"/>
    <many-to-one field="fieldTypes" target-entity="Contentinum\Entity\FieldTypes">
      <join-columns>
        <join-column name="field_types_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
