<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contentinum\Entity\Contacts" table="contacts">
    <indexes>
      <index name="ACCOUNTIDENT" columns="account_ident"/>
      <index name="CONTACTNAME" columns="last_name"/>
      <index name="ACCOUNTSCOPE" columns="accounts_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="NONE"/>
    </id>
    <field name="accountIdent" type="string" column="account_ident" length="36" nullable="false"/>
    <field name="fieldmetaId" type="integer" column="fieldmeta_id" nullable="false"/>
    <field name="title" type="string" column="title" length="5" nullable="false"/>
    <field name="salutation" type="string" column="salutation" length="10" nullable="false"/>
    <field name="firstName" type="string" column="first_name" length="100" nullable="false"/>
    <field name="lastName" type="string" column="last_name" length="100" nullable="false"/>
    <field name="businessTitle" type="string" column="business_title" length="100" nullable="false"/>
    <field name="department" type="string" column="department" length="255" nullable="false"/>
    <field name="phoneHome" type="string" column="phone_home" length="25" nullable="false"/>
    <field name="phoneMobile" type="string" column="phone_mobile" length="25" nullable="false"/>
    <field name="phoneWork" type="string" column="phone_work" length="25" nullable="false"/>
    <field name="phoneOther" type="string" column="phone_other" length="25" nullable="false"/>
    <field name="phoneFax" type="string" column="phone_fax" length="25" nullable="false"/>
    <field name="contactEmail" type="string" column="contact_email" length="100" nullable="false"/>
    <field name="contactChat" type="string" column="contact_chat" length="100" nullable="false"/>
    <field name="contactImgSource" type="string" column="contact_img_source" length="250" nullable="false"/>
    <field name="contactImgLarge" type="string" column="contact_img_large" length="250" nullable="false"/>
    <field name="birthdate" type="date" column="birthdate" nullable="false"/>
    <field name="publish" type="string" column="publish" length="10" nullable="false"/>
    <field name="deleted" type="boolean" column="deleted" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <field name="updateBy" type="integer" column="update_by" nullable="false"/>
    <field name="registerDate" type="datetime" column="register_date" nullable="false"/>
    <field name="upDate" type="datetime" column="up_date" nullable="false"/>
    <many-to-one field="accounts" target-entity="Contentinum\Entity\Accounts">
      <join-columns>
        <join-column name="accounts_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
