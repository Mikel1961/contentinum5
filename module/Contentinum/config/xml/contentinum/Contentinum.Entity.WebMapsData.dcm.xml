<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contentinum\Entity\WebMapsData" table="web_maps_data">
    <indexes>
      <index name="MAPIDENTREF" columns="web_maps_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="NONE"/>
    </id>
    <field name="latitude" type="string" column="latitude" length="50" nullable="false"/>
    <field name="longitude" type="string" column="longitude" length="50" nullable="false"/>
    <field name="name" type="string" column="name" length="250" nullable="false"/>
    <field name="street" type="string" column="street" length="250" nullable="false"/>
    <field name="city" type="string" column="city" length="250" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="imgSource" type="string" column="img_source" length="100" nullable="false"/>
    <field name="publish" type="string" column="publish" length="10" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <field name="updateBy" type="integer" column="update_by" nullable="false"/>
    <field name="registerDate" type="datetime" column="register_date" nullable="false"/>
    <field name="upDate" type="datetime" column="up_date" nullable="false"/>
    <many-to-one field="webMaps" target-entity="Contentinum\Entity\WebMaps">
      <join-columns>
        <join-column name="web_maps_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
