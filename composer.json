{
	"name" : "contentinum/content-managment",
	"description" : "Contentinum content managment system in version 5",
	"type" : "application",
	"authors" : [{
			"name" : "Michael Jochum",
			"email" : "michael.jochum@jochum-mediaservices.de",
			"homepage" : "http://jochum-mediaservices.de/"
		}
	],
	"keywords" : [
		"contentinum",
		"cms"
	],
	"homepage" : "https://github.com/Mikel1961/contentinum5",
	"license" : [
		"BSD"
	],
	"require" : {
		"doctrine/doctrine-orm-module" : "0.*",
		"widmogrod/zf2-assetic-module" : "1.*",
		"kriswallsmith/assetic": "1.1.*",
		"contentinum/contentinum-components" : "dev-master",
		"php" : ">=5.3.3",
		"zendframework/zendframework" : "2.2.*"
	},
	"autoload" : {
		"psr-0" : {
			"Contentinum" : "module/Contentinum/src",
			"Mcuser" : "module/Mcuser/src/",
			"Mcevent" : "module/Mcevent/src/",
			"Mcwork" : "module/Mcwork/src/"
		}
	},
	"minimum-stability" : "dev",
	"replace" : {
		"content-managment/contentinum-users" : "self.version",
		"content-managment/contentinum-backend" : "self.version",
		"content-managment/contentinum" : "self.version",
		"content-managment/contentinum-events" : "self.version"
	}
}