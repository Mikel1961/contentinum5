<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<default name="Layout HTML5 Boilerplate">
		<sequence>
			<action row="10" set="section">
				<param type="role">
					<value>main</value>
				</param>
			</action>
			<action row="20" set="assign">
				<param>
					<value>allcontent</value>
				</param>
			</action>

			<action row="40" set="boxend">
				<param>
					<value>section</value>
				</param>
			</action>
		</sequence>
		<header>
			<styles>
				<assets>1</assets>
			</styles>
			<scripts>
				<assets>1</assets>
			</scripts>
		</header>
		<footer>
			<assets>1</assets>
		</footer>
		<alternative>
			<!-- alternate adjustments -->
		</alternative>
	</default>
	<foundation name="Layout Foundation based">
		<sequence>
			<action row="20" set="div">
				<param type="class">
					<value>row</value>
				</param>
			</action>
			<action row="40" set="div">
				<param type="class">
					<value>large-12 columns</value>
				</param>
			</action>

			<action row="100" set="assign">
				<param>
					<value>allcontent</value>
				</param>
			</action>

			<action row="120" set="boxend">
				<param>
					<value>div</value>
				</param>
			</action>
			<action row="140" set="boxend">
				<param>
					<value>div</value>
				</param>
			</action>
		</sequence>
		<header>
			<styles>
				<assets>1</assets>
			</styles>
			<scripts>
				<assets>1</assets>
			</scripts>
		</header>
		<footer>
			<assets>1</assets>
		</footer>
		<alternative>
			<!-- alternate adjustments -->
		</alternative>
	</foundation>
</layout>